<?xml version="1.0" encoding="utf-8" ?>
<!-- Zawiera informację nagłówkową pliku XML oraz informację o użytym kodowaniu znaków. -->
<module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		xmlns="http://www.enova.pl/schema/business_struct.xsd"
		xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/business_struct.xsd">
	<table name="APSReportMRPPosition"
		   tablename="APSReportMRP">
		<col name="ReportMRPElement"
			type="APSReportMRPElement"
			children="Positions"
			delete="cascade"
			readonly="true"
			required="true"
			modifier="public virtual"
			caption="Element raportu MRP" />
		<col name="Quantity"
			type="Quantity"
			modifier="public virtual"
			caption="Ilość" />
		<col name="Direction"
			type="KierunekPartii"
			modifier="public virtual"
			caption="Kierunek" />
		<col name="DefinitionCode"
			type="string"
			length="12"
			modifier="public virtual"
			caption="Kod definicji" />
		<col name="DocumentNumber"
			type="string"
			length="40"
			modifier="public virtual"
			caption="Numer pełny dokumentu" />
		<col name="ObtainingMethod"
			type="ObtainingMethodEnum"
			modifier="public virtual"
			readonly="set"
			caption="Metoda uzyskania" />
		<col name="ObtainingPeriod"
			type="int"
			modifier="public virtual"
			caption="Okres uzyskania (dni)" />
		<col name="StartDate"
			type="date"
			modifier="public virtual"
			caption="Data startu" />
		<col name="AvailabilityDate"
			type="date"
			modifier="public virtual"
			caption="Data dostępności" />
		<col name="BalanceQuantity"
			type="Quantity"
			modifier="public virtual"
			caption="Ilość bilans" />
		<col name="CreatedFromSuggestion"
			type="boolean"
			modifier="public virtual"
			caption="Utworzone z sugestii" />
	</table>
</module>