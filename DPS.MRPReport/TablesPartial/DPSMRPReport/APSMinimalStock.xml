<!-- Zawiera informację nagłówkową pliku XML oraz informację o użytym kodowaniu znaków. -->
<module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		xmlns="http://www.enova.pl/schema/business_struct.xsd"
		xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/business_struct.xsd">
	<table name="APSMinimalStock"
		   tablename="APSMinStocks">
		<col name="Towar"
			type="Towar"
			required="true"
			readonly="true"
			caption="Towar" />
		<col name="Period"
			type="FromTo"
			required="true"
			modifier="public virtual"
			caption="Okres" />
		<col name="Quantity"
			 type="Quantity"
			 modifier="public virtual"
			 caption="Ilość" />
