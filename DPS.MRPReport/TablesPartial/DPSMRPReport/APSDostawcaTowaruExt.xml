<?xml version="1.0" encoding="utf-8" ?>
<!-- Zawiera informację nagłówkową pliku XML oraz informację o użytym kodowaniu znaków. -->
<module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		xmlns="http://www.enova.pl/schema/business_struct.xsd"
		xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/business_struct.xsd">
	<table name="APSDostawcaTowaruExt"
		   tablename="APSDostTowExt">
		<col name="DostawcaTowaru"
			type="DostawcaTowaru"
			keyprimary="true"
			keyunique="true"
			delete="cascade"
			relguided="inner"
			relright="true"
			readonly="true"
			required="true" />
		<col name="Priority"
			type="double"
			modifier="public virtual"
			caption="Priorytet" />
		<col name="QualityAssessment"
			type="QualityAssessmentEnum"
			modifier="public virtual"
			caption="Ocena jakości" />
		<col name="LogisticalMinimum"
			type="Quantity"
			modifier="public virtual"
			caption="Minimum logistyczne" />
		<col name="Description"
			type="string"
			length="400"
			modifier="public virtual"
			caption="Opis" />
	</table>
</module>